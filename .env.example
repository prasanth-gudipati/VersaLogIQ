# VersaLogIQ Environment Configuration
# Copy this file to .env and customize as needed

# Application Settings
FLASK_ENV=production
PYTHONUNBUFFERED=1
LOG_LEVEL=INFO

# Default Connection Settings (optional - can be changed in UI)
DEFAULT_HOST=192.168.1.100
DEFAULT_USERNAME=admin

# Docker Settings
COMPOSE_PROJECT_NAME=versalogiq

# Network Settings
BACKEND_PORT=5000
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443
REDIS_PORT=6379

# Volume Settings
LOGS_VOLUME=./backend/logs
CONFIG_VOLUME=./config